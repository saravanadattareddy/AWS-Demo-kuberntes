    2  yum install git
    3  git clone https://github.com/saravanadattareddy/AWS-Demo-kuberntes.git master
    4  ls
    5  cd master
    6  ls
    7  git checkout master
    8  git branch
    9  git clone https://github.com/saravanadattareddy/AWS-Demo-kuberntes.git
   10  ls
   11  yum install httpd
   12  cp /var/www/html

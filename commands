   16  cp -r error/ /var/www/html
   17  cp -r images/ /var/www/html
   18  cp -r index.html/ /var/www/html
   19  cp  index.html /var/www/html
   20  cd
   21  cd /var/www/html
   22  ls
   23  systemctl start httpd
   24  chkconfig httpd
   25  yum install nodejs -y
